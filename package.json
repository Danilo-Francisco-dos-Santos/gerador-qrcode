{
  "name": "gerador-qrcode",
  "productName": "Gerador de QRCode",
  "version": "1.0.0",
  "description": "Gerador de QRCode por API",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder",
    "package": "electron-builder -p always",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "build": {
    "appId": "gerador-qrcode",
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "image/icone.ico",
      "requestedExecutionLevel": "requireAdministrator"
    },
    "nsis": {
      "installerIcon": "image/icone.ico",
      "uninstallerIcon": "image/icone.ico",
      "uninstallDisplayName": "Gerador de QRCode",
      "license": "license.md",
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "publish": [
      {
        "provider": "",
        "owner": "",
        "repo": "",
        "token": "",
        "private": ,
        "releaseType": ""
      }
    ]
  },
  "author": "Danilo Santos",
  "license": "MIT",
  "devDependencies": {
    "electron": "^34.0.0",
    "electron-builder": "^25.1.8"
  },
  "dependencies": {
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "electron-updater": "^6.3.9"
  }
}